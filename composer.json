{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "barryvdh/laravel-dompdf": "^2.2",
        "bezhansalleh/filament-shield": "^3.2",
        "directorytree/ldaprecord-laravel": "^3.2",
        "filament/filament": "^3.2",
        "guzzlehttp/guzzle": "^7.3",
        "husam-tariq/filament-database-schedule": "^2.0",
        "laravel/framework": "^10.3",
        "laravel/pulse": "^1.0@beta",
        "laravel/sanctum": "^3.0",
        "laravel/telescope": "^5.2",
        "laravel/tinker": "^2.9",
        "pxlrbt/filament-excel": "^2.3",
        "spatie/laravel-permission": "^6.3",
        "stechstudio/filament-impersonate": "^3.6",
        "uisits/laravel-oidc": "^10.0",
        "yajra/laravel-oci8": "^10.4"
    },
    "require-dev": {
        "fakerphp/faker": "^1.9.1",
        "jasonmccreary/laravel-test-assertions": "^2.3",
        "laravel-shift/blueprint": "^2.5",
        "laravel/pint": "^1.0",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^7.1",
        "phpunit/phpunit": "^9.6",
        "spatie/laravel-ignition": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "@php artisan filament:upgrade"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "beta",
    "prefer-stable": true
}
